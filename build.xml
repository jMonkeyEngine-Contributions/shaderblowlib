<project name="shaderblow" default="netbeans" basedir=".">
    <description>Builds, tests, and runs the project shaderblow.</description>
    <import file="nbproject/build-impl.xml"/>
	
	<target name="init" depends="basic-init,files-init,build-init,-javac-init,-build-subproject"/>
